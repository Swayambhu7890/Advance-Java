<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
    <class name="test.Question" table="Question123">

        <id name="id" type = "int">
            <column name = "qid"/>
            <generator class="increment"></generator>
        </id>

         <property name="qname"></property>

        <list name="answers" table="ques_ans_123" fetch = "select" cascade="all">         <!-- fetch :- To select the data from table (cascade:-merge all)-->
              <key column="qid"/>
              <index column="type"/>
              <many-to-many class = "test.Answer" column = "ansid"></many-to-many>
        </list>
        
    </class>

</hibernate-mapping>
